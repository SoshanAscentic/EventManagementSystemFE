{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/assets/*", "/static/*", "/*.css", "/*.js", "/*.ico", "/*.png", "/*.svg", "/*.json", "/*.txt", "/api/*"]
  },
  "mimeTypes": {
    ".js": "text/javascript",
    ".json": "application/json"
  },
  "globalHeaders": {
    "cache-control": "must-revalidate, max-age=6000"
  },
  "trailingSlash": "never",
  "routes": [
    {
      "route": "/assets/*",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    }
  ]
}